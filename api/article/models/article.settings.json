{
  "connection": "default",
  "collectionName": "articles",
  "info": {
    "name": "article"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "body": {
      "type": "richtext",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "photos": {
      "collection": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "brief": {
      "type": "string",
      "required": true
    },
    "tags": {
      "collection": "tag"
    }
  }
}
